{
    "configurations": [{
        "name": "arm_c_cpp_properties",
        "compilerPath": "${config:GCCPath}/bin/arm-none-eabi-gcc.exe",
        "includePath": [
            "${workspaceFolder}/Src",
            "${workspaceFolder}/Inc",
            "${workspaceFolder}/Lib/**",
            "${workspaceFolder}/Lib/canDriver/**",
            "${workspaceFolder}/Lib/Debug/**",
            "${workspaceFolder}/Lib/canfestival/src",
            "${workspaceFolder}/Lib/canfestival/include",
            "${workspaceFolder}/Lib/canfestival/include/cm3",
            "${workspaceFolder}/Drivers/**",
            "${workspaceFolder}/Middlewares/**",
